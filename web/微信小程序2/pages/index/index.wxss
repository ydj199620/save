.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  /* 弹性盒子里每一项 */
}

.title {
  font-size: 26rpx;
  margin-top: 40rpx;
  font-weight: 700;
  color: #000;
}

.logo {
  width: 392rpx;
  height: 280rpx;
  margin-top: 80rpx;
  /* rpx是微信短像素单位 */
}

.btn {
  border: 1px solid #000;
  border-radius: 5px;
  width: 300rpx;
  height: 80rpx;
  text-align: center;
  margin-top: 80rpx;
}

.music_btn {
  font-size: 24rpx;
  font-weight: 700;
  line-height: 80rpx;
  color: #000;
}

.btn2 {
  margin-top: 50rpx;
}
